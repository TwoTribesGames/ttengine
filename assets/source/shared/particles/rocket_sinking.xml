<?xml version="1.0" encoding="iso-8859-1"?>
<ParticleTrigger xmlns="http://intranet.twotribes.com/schemas/common/particletrigger.xsd" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://intranet.twotribes.com/schemas/common/particletrigger.xsd http://intranet.twotribes.com/schemas/common/particletrigger.xsd">
	<GameEvent name="rocket_sinking" max_particles="300"/>
	<ParticleEmitter max_particles="20" x="0" y="0" delay="0" space="world">
		<RenderGroup value="300"/>
		<Texture filename="particles/water_bubble.png"/>
		<FrameAnimations framesize="0">
			<Animation start="0" end="0" fps="30" type="loop"/>
		</FrameAnimations>
		<EmissionCircle radius="0.3" innerradius="0"/>
		<ExternalForce min_x="0" max_x="0" min_y="0.1" max_y="0.1"/>
		<BlendMode type="add"/>
		<PreGeneration time="0" step="0"/>
		<Emission particles="20" type="continuous" lifetime="0"/>
		<ParticleOrientation type="rotation"/>
		<Velocity min_x="0" max_x="0" min_y="5" max_y="5"/>
		<VelocityFriction min="0.07" max="0.17"/>
		<ParticleLife min="0.75" max="1.25"/>
		<Size min_start="0.1" max_start="0.3" min_end="0.075" max_end="0.225"/>
		<Weight min_start="0" max_start="0" min_end="1" max_end="1"/>
		<Rotation min_start="0" max_start="0" min_speed="0" max_speed="0"/>
		<RotationForce min="0" max="0"/>
		<RotationFriction min="0" max="0"/>
		<Frame min_start="0" max_start="4"/>
		<DirectionFromSpawnLocation/>
		<MinStartColor r="133" g="230" b="255" a="255"/>
		<MaxStartColor r="255" g="255" b="255" a="255"/>
		<MinEndColor r="255" g="255" b="255" a="255"/>
		<MaxEndColor r="137" g="207" b="251" a="255"/>
		<Fade in="0.1" out="0.4" type="percentage"/>
	</ParticleEmitter>
</ParticleTrigger>
